{
    "multipart": [
        {   "apply": { "model": "voltaics:lowVoltageConduitCenter" }},
        {   "when": { "up": "true" },
            "apply": { "model": "voltaics:lowVoltageConduitSide", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "voltaics:lowVoltageConduitSide", "x": 90, "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "voltaics:lowVoltageConduitSide", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "voltaics:lowVoltageConduitSide", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "voltaics:lowVoltageConduitSide", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "voltaics:lowVoltageConduitSide", "y": 270, "uvlock": true }
        }
    ]
}

